{
	"name": "@open-vanilla/result",
	"version": "1.0.0",
	"description": "A vanilla module implementing the Result (also known as Either) pattern to make the error handling more explicit",
	"homepage": "https://github.com/adbayb/esvisitor/tree/main/esvisitor#readme",
	"bugs": "https://github.com/adbayb/esvisitor/issues",
	"repository": {
		"type": "git",
		"url": "git@github.com:adbayb/open-vanilla.git",
		"directory": "libraries/visitor"
	},
	"license": "MIT",
	"author": "Ayoub Adib <adbayb@gmail.com> (https://twitter.com/adbayb)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"source": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs",
			"default": "./dist/index.mjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm clean && quickbundle build",
		"clean": "rm -rf dist",
		"prepublishOnly": "pnpm build",
		"test": "vitest --typecheck --reporter=verbose",
		"watch": "quickbundle watch"
	},
	"devDependencies": {
		"quickbundle": "2.8.0",
		"vitest": "2.1.8"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
